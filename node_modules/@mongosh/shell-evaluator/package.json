{
  "name": "@mongosh/shell-evaluator",
  "version": "1.8.0",
  "description": "MongoDB Top Level API Package",
  "main": "./lib/index.js",
  "scripts": {
    "test": "mocha -r \"../../scripts/import-expansions.js\" --timeout 60000 -r ts-node/register \"./{src,lib}/**/*.spec.ts\"",
    "test-ci": "node ../../scripts/run-if-package-requested.js npm test",
    "lint": "eslint --report-unused-disable-directives \"./{src,test}/**/*.{js,ts,tsx}\"",
    "check": "npm run lint && depcheck --skip-missing=true",
    "compile-ts": "tsc -p tsconfig.json",
    "prepublish": "npm run compile-ts"
  },
  "config": {
    "unsafe-perm": true
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mongodb-js/mongosh.git"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.15.1"
  },
  "mongosh": {
    "unitTestsOnly": true
  },
  "devDependencies": {
    "@types/sinon-chai": "^3.2.3"
  },
  "dependencies": {
    "@mongosh/async-rewriter2": "1.8.0",
    "@mongosh/history": "1.8.0",
    "@mongosh/shell-api": "1.8.0"
  },
  "gitHead": "9cf53bc336c79e505cf034bf5e6f3b3b3796cf25"
}
